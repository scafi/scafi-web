<xml xmlns="https://developers.google.com/blockly/xml" id="toolbox" style="display: none">
    <category name="Basic" colour="#a5745b">
        <block type="output"></block>
    </category>
    <category name="Conditions" colour="#9fa55b">
        <block type="branch"></block>
        <block type="mux"></block>
    </category>
    <category name="Building Blocks" colour="#5BA55B">
        <block type="leaderElection"></block>
        <block type="distance_to">
            <value name="SRC">
                <block type="sense">
                    <value name="TYPE">
                        <block type="class_boolean">
                            <field name="NAME">Boolean</field>
                        </block>
                    </value>
                    <value name="SENSOR_NAME">
                        <block type="string_value">
                            <field name="VALUE">source</field>
                        </block>
                    </value>
                </block>
            </value>
        </block>
        <block type="distance_between">
            <value name="SOURCE">
                <block type="sense">
                    <value name="TYPE">
                        <block type="class_boolean">
                            <field name="NAME">Boolean</field>
                        </block>
                    </value>
                    <value name="SENSOR_NAME">
                        <block type="string_value">
                            <field name="VALUE">source</field>
                        </block>
                    </value>
                </block>
            </value>
            <value name="TARGET">
                <block type="sense">
                    <value name="TYPE">
                        <block type="class_boolean">
                            <field name="NAME">Boolean</field>
                        </block>
                    </value>
                    <value name="SENSOR_NAME">
                        <block type="string_value">
                            <field name="VALUE">target</field>
                        </block>
                    </value>
                </block>
            </value>
        </block>
    </category>
    <category name="User-API" colour="#A55BA5">
        <block type="channel">
            <value name="SOURCE">
                <block type="sense">
                    <value name="TYPE">
                        <block type="class_boolean">
                            <field name="NAME">Boolean</field>
                        </block>
                    </value>
                    <value name="SENSOR_NAME">
                        <block type="string_value">
                            <field name="VALUE">source</field>
                        </block>
                    </value>
                </block>
            </value>
            <value name="TARGET">
                <block type="sense">
                    <value name="TYPE">
                        <block type="class_boolean">
                            <field name="NAME">Boolean</field>
                        </block>
                    </value>
                    <value name="SENSOR_NAME">
                        <block type="string_value">
                            <field name="VALUE">target</field>
                        </block>
                    </value>
                </block>
            </value>
        </block>
    </category>

    <category name="Sensing" colour="#5b67a5">
        <block type="mid"></block>
        <block type="sense">
            <value name="TYPE">
                <block type="class_boolean">
                    <field name="NAME">Boolean</field>
                </block>
            </value>
            <value name="SENSOR_NAME">
                <block type="string_value">
                    <field name="VALUE">sensorX</field>
                </block>
            </value>
        </block>
    </category>
    <category name="Actuation" colour="#A55B5B">
        <block type="led_all_to">
            <value name="COLOR">
                <block type="color_value">
                    <field name="VALUE">#ffff00</field>
                </block>
            </value>
        </block>
        <block type="velocity"></block>
    </category>
    <category name="Movement" colour="#5b67a5">
        <block type="random_movement"></block>
        <block type="stand_still"></block>
        <block type="spin_around"></block>
        <block type="flocking"></block>
        <block type="sink_at"></block>
        <block type="line"></block>
        <block type="centered_circle"></block>
    </category>
    <category name="Operations" colour="#745ba5">
        <block type="equals"></block>
        <block type="not"></block>
        <block type="boolean_operation">
            <field name="OPERATOR">&amp;&amp;</field>
        </block>
        <block type="number_compare">
            <field name="OPERATOR">GREATER</field>
        </block>
        <block type="number_operation">
            <field name="OPERATOR">ADDITION</field>
        </block>
        <block type="ternary_operation">
        </block>
    </category>
    <category name="Values" colour="#995ba5">
        <block type="string_value">
            <field name="VALUE"></field>
        </block>
        <block type="integer_value">
            <field name="VALUE">1</field>
        </block>
        <block type="double_value">
            <field name="VALUE">NaN</field>
        </block>
        <block type="boolean_value">
            <field name="VALUE">true</field>
        </block>
        <block type="color_value">
            <field name="VALUE">#ffff00</field>
        </block>
        <block type="hsl">
            <value name="VALUE_1">
                <block type="integer_value">
                    <field name="VALUE">128</field>
                </block>
            </value>
            <value name="VALUE_2">
                <block type="integer_value">
                    <field name="VALUE">128</field>
                </block>
            </value>
            <value name="VALUE_3">
                <block type="integer_value">
                    <field name="VALUE">128</field>
                </block>
            </value>
        </block>

    </category>
    <category name="Types" colour="#a55b80">
        <block type="class_integer">
            <field name="NAME">Integer</field>
        </block>
        <block type="class_double">
            <field name="NAME">Double</field>
        </block>
        <block type="class_boolean">
            <field name="NAME">Boolean</field>
        </block>
        <block type="class_string">
            <field name="NAME">String</field>
        </block>
        <block type="class_other">
            <field name="NAME"></field>
        </block>
    </category>

    <category name="Definitions" colour="#9fa55b" custom="Definitions">
        <block type="define">
            <field name="NAME">x</field>
        </block>
        <block type="val">
            <field name="NAME">x</field>
        </block>
        <block type="var">
            <field name="NAME">x</field>
        </block>
    </category>
</xml>